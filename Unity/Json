[System.Serializable]
public class DataInfo
{
    public DataAttribute[] database;
}

[System.Serializable]
public class DataAttribute
{
    public string name;
    public string path;
}

class Database
{
     public Database()
     {
          string dataText = Resources.Load<TextAsset>("Database").text;
          DataInfo infodata;
          infodata = JsonUtility.FromJson<DataInfo>(dataText);
          data = infodata.database;
